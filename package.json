{
  "name": "@mockoon/commons-server",
  "description": "Mockoon's commons server library. Used in Mockoon desktop application and CLI.",
  "version": "2.10.0",
  "author": {
    "name": "Guillaume Monnet",
    "email": "hi@255kb.dev",
    "url": "https://255kb.dev/"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .js,.ts .",
    "format": "prettier {src,test}/**/*.{ts,js,scss,html} --check",
    "test": "ts-mocha -p ./tsconfig-build-cjs.json --timeout 10000 --forbid-only test/**/*.spec.ts",
    "build": "tsc -p tsconfig-build.json && tsc -p tsconfig-build-cjs.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockoon/commons-server.git"
  },
  "homepage": "https://github.com/mockoon/commons-server#readme",
  "funding": "https://mockoon.com/sponsor-us/",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@mockoon/commons": "2.5.0",
    "bson-objectid": "2.0.1",
    "cookie-parser": "1.4.5",
    "date-fns": "2.22.1",
    "express": "4.17.1",
    "faker": "5.5.3",
    "handlebars": "4.7.7",
    "http-proxy-middleware": "2.0.0",
    "killable": "1.0.1",
    "mime-types": "2.1.31",
    "object-path": "0.11.5",
    "qs": "6.10.1",
    "typed-emitter": "1.3.1",
    "uuid": "8.3.2",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@types/chai": "4.2.19",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.12",
    "@types/faker": "5.5.6",
    "@types/lodash": "4.14.170",
    "@types/mime-types": "2.1.0",
    "@types/mocha": "8.2.2",
    "@types/node": "15.12.5",
    "@types/object-path": "0.11.0",
    "@types/qs": "6.9.6",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "chai": "4.3.4",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsdoc": "35.4.1",
    "mocha": "9.0.1",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "2.2.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.3.5"
  }
}
